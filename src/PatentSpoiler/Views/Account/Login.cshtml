@model dynamic

@{
    ViewBag.Title = "login";
}

<h2>Log in!</h2>

<form method="POST">
    @Html.AntiForgeryToken()
    Username: @Html.TextBox("Username")<br/>
    Password: @Html.Password("Password")<br/>
    Remember me @Html.CheckBox("RememberMe")<br />
    <input type="submit" value="Log in"/>
</form>